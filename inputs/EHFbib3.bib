	keywords = {experiments, spite, Identity, Polarization, Norms, Nudging, Social Preferences},
	language = {en},
	month = jun,
	shorttitle = {Hate {Trumps} {Love}},
	title = {Hate {Trumps} {Love}: {The} {Impact} of {Political} {Polarization} on {Social} {Preferences}},
	type = {{SSRN} {Scholarly} {Paper}},
	url = {https://papers.ssrn.com/abstract=3680871},
