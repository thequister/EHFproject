	keywords = {insurance, UI, risk, unemployment},
	language = {en},
	month = mar,
	note = {Publisher: Cambridge University Press},
	number = {1},
	pages = {23--49},
	shorttitle = {The {Insecure} {American}},
	title = {The {Insecure} {American}: {Economic} {Experiences}, {Financial} {Worries}, and {Policy} {Attitudes}},
	url = {https://www.cambridge.org/core/journals/perspectives-on-politics/article/insecure-american-economic-experiences-financial-worries-and-policy-attitudes/3E0C49C592FDD3D3238FEF7859D7399A},
