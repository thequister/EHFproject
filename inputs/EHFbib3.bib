@article{vuillemey_evading_nodate,
	abstract = {I show that the maritime shipping industry {\textendash} handling above 80\% of global trade flows {\textendash} has evolved over the past decades to systematically evade {\textquotedblleft}corporate responsibilities,{\textquotedblright} i.e., compliance with regulatory standards and potential tort liabilities. Shipping firms increasingly dissociated legal and ultimate ownership, fragmented assets in one-ship subsidiaries, used flags of convenience, and evaded end-of-life responsibilities with {\textquotedblleft}last-voyage flags.{\textquotedblright} Microeconomic tests confirm that responsibility evasion, amidst global competition, is a dominant motive behind these patterns. These findings have implications for our understanding of corporate social responsibility, of extended forms of liability, and of the {\textquotedblleft}dark side{\textquotedblright} of globalization.},
	author = {Vuillemey, Guillaume},
