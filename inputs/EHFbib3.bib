	journal = {Perspectives on Politics},
	keywords = {unions, USA, representation},
	language = {en},
	note = {Publisher: Cambridge University Press},
	pages = {1--18},
	shorttitle = {Reducing {Unequal} {Representation}},
	title = {Reducing {Unequal} {Representation}: {The} {Impact} of {Labor} {Unions} on {Legislative} {Responsiveness} in the {U}.{S}. {Congress}},
	url = {https://www.cambridge.org/core/journals/perspectives-on-politics/article/reducing-unequal-representation-the-impact-of-labor-unions-on-legislative-responsiveness-in-the-us-congress/D72A0BDB4EA513DFE988331D5380E8DE},
