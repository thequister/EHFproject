@article{colgan_asset_nodate,
	abstract = {Whereas scholars have typically modeled climate change as a global collective action challenge, we offer a dynamic theory of climate politics based on the present and future revaluation of assets. Climate politics can be understood as a contest between owners of assets that accelerate climate change, such as fossil fuel plants, and owners of assets vulnerable to climate change, such as coastal property. To date, obstruction by {\textquotedblleft}climate-forcing{\textquotedblright} asset holders has been a large barrier to effective climate policy. But as climate change and decarbonization policies proceed, holders of both climate-forcing and {\textquotedblleft}climate-vulnerable{\textquotedblright} assets stand to lose some or even all of their assets' value over time, and with them, the basis of their political power. This dynamic contest between opposing interests is likely to intensify in many sites of political contestation, from the subnational to transnational levels. As it does so, climate politics will become increasingly existential, potentially reshaping political alignments within and across countries. Such shifts may further undermine the Liberal International Order (LIO); as countries develop pro-climate policies at different speeds and magnitudes, they will have incentives to diverge from existing arrangements over trade and economic integration.},
	author = {Colgan, Jeff D. and Green, Jessica F. and Hale, Thomas N.},
	doi = {10.1017/S0020818320000296},
	file = {Colgan et al_undefineded_Asset Revaluation and the Existential Politics of Climate Change.pdf:/Users/thequist/Dropbox/ZoteroPDF/Colgan et al_undefineded_Asset Revaluation and the Existential Politics of Climate Change.pdf:application/pdf;Snapshot:/Users/thequist/Zotero/storage/8XHNKNYF/0963988860A37F6988E73738EA93E0A1.html:text/html},
	issn = {0020-8183, 1531-5088},
	journal = {International Organization},
	keywords = {trade, Asset revaluation, climate, climate change, decarbonization, existential politics, liberal international order (LIO)},
	language = {en},
	note = {Publisher: Cambridge University Press},
	pages = {1--25},
	title = {Asset {Revaluation} and the {Existential} {Politics} of {Climate} {Change}},
	url = {https://www.cambridge.org/core/journals/international-organization/article/asset-revaluation-and-the-existential-politics-of-climate-change/0963988860A37F6988E73738EA93E0A1},
	urldate = {2020-12-24},
	bdsk-url-1 = {https://www.cambridge.org/core/journals/international-organization/article/asset-revaluation-and-the-existential-politics-of-climate-change/0963988860A37F6988E73738EA93E0A1},
