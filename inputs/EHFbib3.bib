	number = {3},
	pages = {379--391},
	title = {Resampling-based {Variance} {Estimation} for {Labour} {Force} {Surveys}},
	url = {https://rss.onlinelibrary.wiley.com/doi/abs/10.1111/1467-9884.00196},
	urldate = {2021-08-23},
