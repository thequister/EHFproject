	institution = {Social Science Research Network},
	keywords = {institutions, blockchain, \_tablet, blockchains, NFTs, pluralism, soulbound tokens, Web3, mechanism design, web3},
	language = {en},
	month = may,
	number = {4105763},
	shorttitle = {Decentralized {Society}},
