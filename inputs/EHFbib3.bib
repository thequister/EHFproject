@article{adhvaryu_expectations_nodate,
	abstract = {Hirschman{\textquoteright}s (1970) seminal thesis that enabling worker {\textquotedblleft}voice{\textquotedblright} prevents exit from the
employment relationship has played a foundational role in labor economics. We provide the
first experimental test of this hypothesis in a real-world setting via a randomized controlled
trial in Indian garment factories. Just after what proved to be a disappointing wage hike,
workers were chosen at random to participate in an anonymous survey in which they were
asked for feedback on job conditions, supervisor performance, and overall job satisfaction.
Enabling voice in this manner reduced turnover and absenteeism after the hike, particularly
for the most disappointed worker},
	author = {Adhvaryu, Achyuta and Molina, Teresa and Nyshadham, Anant},
	journal = {Economic Journal},
	keywords = {labor, firms, jobs, India, voice, Daedalus},
	title = {Expectations, {Wage} {Hikes}, and {Worker} {Voice}}}

@article{adhvaryu_sotto_nodate,
	abstract = {We conducted a randomized controlled trial in which we sought to enhance worker voice through
a mobile-phone-based technology that enabled direct, anonymous communication with the em-
ployer. While treated workers were much more likely to know about the tool, usage was low:
only 5\% of treated workers reported ever using it. Despite this, quit rates and absenteeism were
10\% and 5\% lower in the treatment group than in the control group. Together these findings
suggest a substantial option value of voice, which can promote positive workplace behaviors even
when workers do not directly avail themselves of the technology},
	author = {Adhvaryu, Achyuta and Gade, Smit and Molina, Teresa and Nyshadham, Anant},
	journal = {Journal of Political Economy},
	title = {Sotto {Voce}: {The} {Impacts} of {Technology} to {Enhance} {Worker} {Voice}}}

