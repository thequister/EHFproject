@article{andrews_informativeness_2020,
	abstract = {We propose a way to formalize the relationship between descriptive analysis and structural estimation. A researcher reports an estimate {\^c} of a structural quantity of interest c that is exactly or asymptotically unbiased under some base model. The researcher also reports descriptive statistics that estimate features $\gamma$ of the distribution of the data that are related to c under the base model. A reader entertains a less restrictive model that is local to the base model, under which the estimate {\^c} may be biased. We study the reduction in worst-case bias from a restriction that requires the reader's model to respect the relationship between c and $\gamma$ specified by the base model. Our main result shows that the proportional reduction in worst-case bias depends only on a quantity we call the informativeness of for {\^c}. Informativeness can be easily estimated even for complex models. We recommend that researchers report estimated informativeness alongside their descriptive analyses, and we illustrate with applications to three recent papers.},
	author = {Andrews, Isaiah and Gentzkow, Matthew and Shapiro, Jesse M.},
	copyright = {{\textcopyright} 2020 The Econometric Society},
	doi = {https://doi.org/10.3982/ECTA16768},
	file = {Andrews et al_2020_On the Informativeness of Descriptive Statistics for Structural Estimates.pdf:/Users/thequist/Dropbox/ZoteroPDF/Andrews et al_2020_On the Informativeness of Descriptive Statistics for Structural Estimates.pdf:application/pdf;Snapshot:/Users/thequist/Zotero/storage/TD8RA34T/ECTA16768.html:text/html},
	issn = {1468-0262},
