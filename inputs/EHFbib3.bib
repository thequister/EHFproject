	file = {Fehr_Fischbacher_2004_Third-party punishment and social norms.pdf:/Users/thequist/Dropbox/ZoteroPDF/Fehr_Fischbacher_2004_Third-party punishment and social norms.pdf:application/pdf;ScienceDirect Snapshot:/Users/thequist/Zotero/storage/Y3TXVECK/S1090513804000054.html:text/html},
