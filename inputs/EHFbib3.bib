	number = {4},
	pages = {729--763},
	title = {Democratic {Careening}},
	url = {https://www.cambridge.org/core/journals/world-politics/article/democratic-careening/E94A34CCE67731A1209E100D97DE08F3},
	urldate = {2022-10-13},
	volume = {65},
