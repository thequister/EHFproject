	keywords = {experiments, norms, collective action, Cooperation, Dilemma, Experiment, Group size effect, Third party punishment},
	language = {en},
	month = jul,
	pages = {395--412},
	title = {Group size effect and over-punishment in the case of third party enforcement of social norms},
