@article{sen_race_2016,
	abstract = {Although understanding the role of race, ethnicity, and identity is central to political science, methodological debates persist about whether it is possible to estimate the effect of something immutable. At the heart of the debate is an older theoretical question: Is race best understood under an essentialist or constructivist framework? In contrast to the {\textquotedblleft}immutable characteristics{\textquotedblright} or essentialist approach, we argue that race should be operationalized as a {\textquotedblleft}bundle of sticks{\textquotedblright} that can be disaggregated into elements. With elements of race, causal claims may be possible using two designs: (a) studies that measure the effect of exposure to a racial cue and (b) studies that exploit within-group variation to measure the effect of some manipulable element. These designs can reconcile scholarship on race and causation and offer a clear framework for future research.},
	author = {Sen, Maya and Wasow, Omar},
	doi = {10.1146/annurev-polisci-032015-010015},
	file = {Sen_Wasow_2016_Race as a Bundle of Sticks.pdf:/Users/thequist/Dropbox/ZoteroPDF/Sen_Wasow_2016_Race as a Bundle of Sticks.pdf:application/pdf},
