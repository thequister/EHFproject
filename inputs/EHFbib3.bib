@article{samuels_lord_nodate,
	abstract = {Conventional wisdom holds that landed elites oppose democratization. Whether they fear rising wages, labor mobility or land redistribution, landowners have historically repressed agricultural workers and sustained autocracy. What might change landowning elites{\textquoteright} preferences for dictatorship and reduce their opposition to democracy? Change requires reducing landowners{\textquoteright} need to maintain political control over labor. This transition occurs when mechanization reduces the demand for agricultural workers, eliminating the need for labor-repressive policies. We explain how the adoption of labor-saving technology in agriculture alters landowners{\textquoteright} political preferences for different regimes, so that the more mechanized the agricultural sector, the more likely is democracy to emerge and survive. Our theoretical argument offers a parsimonious revision to Moore{\textquoteright}s thesis that applies to the global transformation of agriculture since his Social Origins first appeared, and results from our cross-national statistical analyses strongly suggest that a positive relationship between agricultural mechanization and democracy does in fact exist.},
	author = {Samuels, David J. and Thomson, Henry},
	doi = {10.1017/S1537592720002303},
	file = {Samuels_Thomson_undefineded_Lord, Peasant {\textellipsis} and Tractor.pdf:/Users/thequist/Dropbox/ZoteroPDF/Samuels_Thomson_undefineded_Lord, Peasant {\textellipsis} and Tractor.pdf:application/pdf;Snapshot:/Users/thequist/Zotero/storage/B4BBYNDU/fa7c5e053c9936ef179231a40604b88d8eac9957.html:text/html},
