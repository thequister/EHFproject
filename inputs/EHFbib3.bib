	journal = {Political Analysis},
	keywords = {experiments, spite, conflict, behavioral games, economics games, ethnic bias, ethnic preference, experimental economics, misattribution, social psychology},
	language = {en},
	month = jul,
	note = {Publisher: Cambridge University Press},
	number = {3},
	pages = {385--404},
