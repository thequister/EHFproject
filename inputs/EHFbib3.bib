	journal = {Proceedings of the National Academy of Sciences},
	keywords = {experiments, surveys, methods, causal inference, asymptotic P values, crossover randomized experiments, Fisher-exact P values, nonparametric statistics, randomization-based inference, sensitivity analyses},
	language = {en},
	month = aug,
	note = {Publisher: National Academy of Sciences Section: Biological Sciences},
	number = {32},
