	shorttitle = {The {Power} to {Dismiss}},
	title = {The {Power} to {Dismiss}: {Trade} {Unions} and the {Regulation} of {Job} {Security} in {Western} {Europe}},
	url = {https://oxford.universitypressscholarship.com/10.1093/acprof:oso/9780198709237.001.0001/acprof-9780198709237},
