@article{angrist_split-sample_1995,
	abstract = {This article reevaluates recent instrumental variables (IV) estimates of the returns to schooling
in light of the fact that two-stage least squares is biased in the same direction as ordinary least
squares (OLS) even in very large samples. We propose a split-sample instrumental variables
(SSIV) estimator that is not biased toward OLS. SSlV uses one-half of a sample to estimate parameters of the first-stage equation. Estimated first-stage parameters are then used to construct
fitted values and second-stage parameter estimates in the other half sample. SSIV is biased
toward 0, but this bias can be corrected. The splt-sample estimators confirm and reinforce some
previous findings on the returns to schooling but fail to confirm others.},
	author = {Angrist, Joshua D and Krueger, Alan B},
	file = {Angrist and Krueger - 1995 - Split-Sample Instrumental Variables Estimates of t.pdf:/Users/thequist/Zotero/storage/PM3CWHCI/Angrist and Krueger - 1995 - Split-Sample Instrumental Variables Estimates of t.pdf:application/pdf},
	journal = {Journal of Business and Economic Statistics},
	language = {en},
	pages = {15},
	title = {Split-{Sample} {Instrumental} {Variables} {Estimates} of the {Return} to {Schooling}},
	year = {1995}}

@article{canty_resampling-based_1999,
	abstract = {Labour force surveys are conducted to estimate quantities such as the unemployment rate and the number of people in work. Interest is typically both in estimates at a given time and in changes between two successive time points. Calibration of the sample to force agreement with known population margins results in random weights being assigned to each response, but the usual methods of variance estimation do not account for this. This paper describes how resampling methods{\textemdash}the jackknife, jackknife linearization, balanced repeated replication and the bootstrap{\textemdash}can be used to do so. We also discuss issues of implementation, and we compare the methods by simulation based on data from the UK Labour Force Survey. The broad conclusions are these: bootstrap and jackknife linearization are less computer intensive than the other resampling methods for such applications and give better standard errors; {\textquoteleft}standard{\textquoteright} methods can be badly biased downwards; and it is essential to take variability of the weights into account.},
	author = {Canty, Angelo J. and Davison, A. C.},
	doi = {10.1111/1467-9884.00196},
	file = {Canty_Davison_1999_Resampling-based Variance Estimation for Labour Force Surveys.pdf:/Users/thequist/Dropbox/ZoteroPDF/Canty_Davison_1999_Resampling-based Variance Estimation for Labour Force Surveys.pdf:application/pdf;Snapshot:/Users/thequist/Zotero/storage/ITNBE4QD/1467-9884.html:text/html},
	issn = {1467-9884},
	journal = {Journal of the Royal Statistical Society: Series D (The Statistician)},
	keywords = {methods, survey, Balanced repeated replication, Bootstrap, Calibration, Jackknife, Jackknife linearization, Panel survey, Post-stratification, Raking ratio, weighting},
	language = {en},
