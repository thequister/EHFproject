	isbn = {978-0-19-022863-7},
	language = {en},
	month = apr,
	publisher = {Oxford University Press},
	title = {Comparative {Political} {Economy} and {Varieties} of {Macroeconomics}},
	url = {https://oxfordre.com/politics/view/10.1093/acrefore/9780190228637.001.0001/acrefore-9780190228637-e-161},
	urldate = {2020-05-11},
	year = {2020},
