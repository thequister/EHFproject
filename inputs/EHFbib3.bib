	journal = {Michigan State Law Review},
	keywords = {work, future of work, labor, contracts, Covenants Not to Compete, Empirical, non-compete, Noncompetes, Noncompetition Agreements, Survey Data},
	language = {en},
	pages = {369--464},
	shorttitle = {Understanding {Noncompetition} {Agreements}},
	title = {Understanding {Noncompetition} {Agreements}: {The} 2014 {Noncompete} {Survey} {Project}},
	url = {https://papers.ssrn.com/abstract=2799961},
	urldate = {2022-01-24},
