@article{rasmussen_working-class_2017,
	abstract = {Many studies have found that countries with union-administered unemployment insurance have higher rates of unionization than countries with state-administered unemployment insurance. With data going further back in history, this article demonstrates that the introduction of so-called {\textquotedblleft}Ghent systems{\textquotedblright} had no effect on unionization rates. We argue that the Ghent effect identified by the existing literature came about as a result of increasing state subsidization and benefit generosity in the 1950s and 1960s. Exploring the partisan politics of unemployment insurance, we show that progressive Liberals ({\textquotedblleft}Social Liberals{\textquotedblright}) favored Ghent designs while Social Democrats favored state-administered unemployment insurance before the Second World War. We also present some evidence suggesting that Left governments, inheriting Ghent systems that were not of their choosing, promoted state subsidization in the postwar era and thus helped generate the Ghent effect identified by the existing literature.},
	author = {Rasmussen, Magnus and Pontusson, Jonas},
	journal = {Comparative Political Studies},
	keywords = {insurance, institutions, unions},
	title = {Working-{Class} {Strength} by {Institutional} {Design}? {Unionization}, {Partisan} {Politics}, and {Unemployment} {Insurance} {Systems}, 1870 to 2010},
	year = {2017}}

