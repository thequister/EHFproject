	file = {Andreoni et al. - Avoiding the Ask A Field Experiment on Altruism, .pdf:/Users/thequist/Zotero/storage/763EAZRV/Andreoni et al. - Avoiding the Ask A Field Experiment on Altruism, .pdf:application/pdf},
	journal = {journal of political economy},
