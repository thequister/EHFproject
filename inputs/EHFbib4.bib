  keywords = {algorithms,big data,teaching},
  file = {/Users/thequist/Dropbox/ZoteroPDF/Obermeyer et al_2019_Dissecting racial bias in an algorithm used to manage the health of populations.pdf;/Users/thequist/Zotero/storage/AQXVW7EI/447.html;/Users/thequist/Zotero/storage/TD7QMA5Y/447.html}
