  file = {/Users/thequist/Zotero/storage/4PJ6983A/Hermansen et al_undefineded_Characterizing and Assessing Temporal Heterogeneity.pdf;/Users/thequist/Zotero/storage/YBGH5W6J/18E6523636477E755160DBB0EB8E1CCC.html}
